{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/news",
      "dest": "api/app.py"
    },
    {
      "src": "/api/articles",
      "dest": "api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
